{
    "compilerOptions": {
        // "lib": [
        //     "esnext"
        // ],
        "declaration": true,
        "declarationDir": "./dist/types",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "rootDir": "./src",
        "outDir": "dist/esm",
        "target": "ES2021"
    },
    "include": [
        "src"
    ],
    "exclude": [
        "dist",
        "node_modules"
    ]
}
